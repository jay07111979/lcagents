{
  "name": "@lendingclub/lcagents",
  "version": "1.0.0-alpha.1",
  "description": "LendingClub Internal Agent System - Distributed BMAD-Core Agents for GitHub Copilot",
  "main": "dist/index.js",
  "bin": {
    "lcagents": "dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-resources",
    "build:watch": "tsc --watch",
    "copy-resources": "node scripts/copy-resources.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "dev": "npm run build && node dist/cli/index.js",
    "prepublishOnly": "npm run build && npm test",
    "prepack": "npm run build && npm test"
  },
  "keywords": [
    "lendingclub",
    "internal",
    "ai-agents",
    "github-copilot",
    "development-tools",
    "automation",
    "bmad-core",
    "workflow",
    "cli"
  ],
  "author": "LendingClub Engineering Team",
  "license": "UNLICENSED",
  "private": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jmaniLC/lcagents.git"
  },
  "bugs": {
    "url": "https://github.com/jmaniLC/lcagents/issues"
  },
  "homepage": "https://github.com/jmaniLC/lcagents#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "fs-extra": "^11.1.1",
    "yaml": "^2.3.2",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "inquirer": "^8.2.5"
  },
  "devDependencies": {
    "@types/node": "^20.5.0",
    "@types/fs-extra": "^11.0.1",
    "@types/inquirer": "^8.2.6",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^6.4.0",
    "eslint": "^8.47.0",
    "prettier": "^3.0.2",
    "typescript": "^5.1.6",
    "jest": "^29.6.2",
    "@types/jest": "^29.5.4",
    "ts-jest": "^29.1.1"
  },
  "files": [
    "dist/**/*",
    "config/**/*",
    "resources/**/*",
    "scripts/**/*",
    "README.md",
    "INSTALL.md",
    "USAGE.md"
  ],
  "publishConfig": {
    "access": "restricted"
  },
  "lendingclub": {
    "internal": true,
    "team": "engineering",
    "support": "#engineering-tools"
  }
}
